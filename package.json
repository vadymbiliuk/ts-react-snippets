{
  "name": "ts-react-snippets",
  "displayName": "ts-react-snippets",
  "description": "The snippets for TypeScript and React lovers",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.35.0"
  },
  "author": "Vadym Biliuk",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.tsrSnippets"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.tsrSnippets",
        "title": "TS React Snippets"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^6.0.1",
    "eslint-plugin-pretty-imports": "^1.0.2",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "license": "ISC"
}
